<StartMenu>:
    name: "start"

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        BoxLayout:
            #cols:2

            Label:
                text: "Welcome to the Mask Quiz. Click Start to find your perfect mask! Using full screen works best! "
                color: 0.2, 0.1, 1, 1
        Button:
            text: "Change theme"
            pos_hint: {"x":.25, "y":.4}
            size_hint: 0.5, 0.2
            on_press: root.theme(*args)



        Button:
            text: "Start"
            background_color: 1,0,0,1
            pos_hint: {"x":.25, "y":.4}
            size_hint: 0.5, 0.2
            on_release:
                app.root.current = "main"
                root.manager.transition.direction = "up"


<MainWindow>:
    name: "main"

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        padding: 50
        spacing: 50
        #cols:1

        BoxLayout:
            #cols:2

            Label:
                text: "What kind of mask would you like? "
                color: 0.2, 0.1, 1, 1

        Button:
            text: "Disposable"
            background_color: 0.4, 0.2, 1, 1
            pos_hint: {"x":.25, "y":.4}
            size_hint: 0.5, 0.2
            on_press: root.clk(*args)
            on_release:
                app.root.current = "third"
                root.manager.transition.direction = "left"

        Button:
            text: "Reusable"
            background_color: 1, 1, 1, 0.6
            pos_hint: {"x":.25, "y":.4}
            size_hint: 0.5, 0.2
            on_press: root.clk(*args)
            on_release:
                app.root.current = "second"
                root.manager.transition.direction = "left"


